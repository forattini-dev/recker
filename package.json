{
  "name": "recker",
  "version": "1.0.3",
  "description": "AI & DevX focused HTTP client for Node.js 18+",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "sideEffects": false,
  "keywords": [
    "http",
    "client",
    "fetch",
    "axios",
    "ky",
    "got",
    "undici",
    "typescript",
    "promise",
    "retry",
    "cache",
    "hooks",
    "http2",
    "http3",
    "sse",
    "websocket",
    "whois",
    "dns",
    "proxy",
    "ai",
    "scraping",
    "cheerio",
    "html-parser"
  ],
  "author": "fforattini",
  "license": "MIT",
  "homepage": "https://forattini-dev.github.io/recker",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/forattini-dev/recker.git"
  },
  "bugs": {
    "url": "https://github.com/forattini-dev/recker/issues"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/core/client.d.ts",
      "import": "./dist/core/client.js"
    },
    "./testing": {
      "types": "./dist/testing/index.d.ts",
      "import": "./dist/testing/index.js"
    },
    "./presets": {
      "types": "./dist/presets/index.d.ts",
      "import": "./dist/presets/index.js"
    },
    "./plugins/*": {
      "types": "./dist/plugins/*.d.ts",
      "import": "./dist/plugins/*.js"
    },
    "./scrape": {
      "types": "./dist/scrape/index.d.ts",
      "import": "./dist/scrape/index.js"
    },
    "./utils/*": {
      "types": "./dist/utils/*.d.ts",
      "import": "./dist/utils/*.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsc",
    "prepare": "husky",
    "prepublishOnly": "pnpm build",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "bench": "tsx benchmark/index.ts",
    "bench:all": "tsx benchmark/run-all.ts",
    "docs:dev": "docsify serve docs --port 3000",
    "docs:preview": "npx -y docsify-cli serve docs --port 3000 --open",
    "lint": "echo \"No linting configured for this project.\" && exit 0"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "basic-ftp": "^5.0.5",
    "glob": "^13.0.0",
    "ssh2-sftp-client": "^12.0.1",
    "telnet-client": "^2.2.9",
    "undici": "^7.16.0",
    "zod": "^4.1.13"
  },
  "peerDependencies": {
    "cheerio": "^1.0.0",
    "ioredis": "^5.0.0"
  },
  "peerDependenciesMeta": {
    "cheerio": {
      "optional": true
    },
    "ioredis": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/needle": "^3.3.0",
    "@types/node": "^24.10.1",
    "@types/ssh2-sftp-client": "^9.0.5",
    "@types/superagent": "^8.1.9",
    "@vitest/coverage-v8": "^3.2.4",
    "axios": "^1.13.2",
    "cheerio": "^1.0.0",
    "docsify-cli": "^4.4.4",
    "domhandler": "^5.0.3",
    "got": "^14.6.5",
    "husky": "^9.1.7",
    "ky": "^1.14.0",
    "mitata": "^1.0.34",
    "needle": "^3.3.1",
    "superagent": "^10.2.3",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  }
}
